                                <div class="ic-spec">
                                    <div class="ic-spec-section">
                                        <h4 class="ic-spec-section-title">Engine and Transmission</h4>
                                        {% assign custom = product.metafields.custom %}
                                        {% if custom.engine_type != blank %}
                                            <div class="ic-spec-row">
                                                <div class="ic-spec-name">Engine Type</div>
                                                <div class="ic-spec-value">{{ custom.engine_type }}</div>
                                            </div>
                                        {% endif %}
                                        {% if custom.product_cc != blank %}
                                            <div class="ic-spec-row">
                                                <div class="ic-spec-name">Displacement</div>
                                                <div class="ic-spec-value">{{ custom.product_cc }}</div>
                                            </div>
                                        {% endif %}
                                        {% if custom.max_power != blank %}
                                            <div class="ic-spec-row">
                                                <div class="ic-spec-name">Max Power</div>
                                                <div class="ic-spec-value">{{ custom.max_power }}</div>
                                            </div>
                                        {% endif %}
                                        {% if custom.drive_type != blank %}
